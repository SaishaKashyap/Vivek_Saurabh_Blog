<%- include("partials/header"); %>


<h1 class="main-heading">Vivek Saurabh</h1>


<%  posts.forEach(function(post){ %>
<div class="posts">
  <h1><%=post.title%></h1>
  <pre>
  <p>
  <%=post.content.substring(0, 200) + " ..."%>
  <a href="/posts/<%=post._id%>">Read More</a>
  </p>
  </pre>

</div>

<% }) %>



